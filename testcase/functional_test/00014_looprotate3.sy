int A[100][100] = {};
int B[100][100] = {};
int main()
{
    int i = 0,j = 0;
    int p = 134;
    while(i < 100){
        j = 0;
        while(j < 100){
            A[i][j] = (i*j+i/(j+1)+3*i+3*j)%p;
            j = j + 1;
        }
        i = i + 1;
    }

    i = 1; j = 1;
    int S = 0;
    while(1){
        j = 1;
        while(1){
            j = j + 1;
            int t = A[i-1][j] + A[i][j-1] + A[i][j];
            B[i][j] = 5*t;
            if(j >= 100){
                break;
            }
            B[i][j] = B[i][j] + 5*t;
        }
        i = i + 1;
        S = S + i*A[i][1];
        if(i >= 100){
            break;
        }
        S = S + i*A[i][1];
    }
    putint(S);
    putch(10);

    i = 0;
    while(i < 100){
        putarray(100,B[i]);
        i = i + 1;
    }
    return 0;
}