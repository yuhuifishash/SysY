int AllNegative1[55][55];
int n = 55;
int setDiag(int diag) {
    int i = 0;
    while (i < n && i <= diag) {
        if (diag - i < n) { 
            AllNegative1[i][diag-i] = -1;
        }
        i = i + 1;
    }
}
int main () {
    @for (int diag = 0; diag <= n+n-1-1; diag = diag+1) {
        setDiag(diag);
    }
    @for (int i = 0; i < n ; i = i+1) {
        @for (int j = 0; j < n; j = j+1) {
            putint(AllNegative1[0][0]);
        }
        putch(10);
    }
    return 0;
}