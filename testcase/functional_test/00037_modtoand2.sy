const int mod[33] = {998244353,-998244353,2,-2,444,-444,123456789,-123456789,9999991,-9999991,17,-17,50,-50,987654321,-987654321,2147483647,-2147483647,-2147483648,123,-123,77,-77,2000000033,-2000000033,999,-999,100003,-100003,144,-144,65537,-65537};

int main() {
	int n;
	n=getint();
	@for(int i=0;i<n;i=i+1){
		int x;
		x=getint();
		@for(int j=0;j<33;j=j+1){
			int y = x%mod[j];
			if(x%mod[j] < mod[32-j-1]){
				if(y%2==1){
					if(y<0){
						putf("%d ",y+1);
					}else{
						putf("%d ",y-1);
					}
				}
				if(y%2!=1){
					if(y<0){
						putf("%d ",y+2);
					}else{
						putf("%d ",y-2);
					}
				}
				
			}else{
				if(y%2!=0){
					if(y<0){
						putf("%d ",y+3);
					}else{
						putf("%d ",y-3);
					}
				}
				if(y%2==0){
					if(y<0){
						putf("%d ",y+4);
					}else{
						putf("%d ",y-4);
					}
				}
			}
		}
		putf("\n");
	}
	
    return 0;
}